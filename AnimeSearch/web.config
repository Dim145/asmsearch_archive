<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <!-- To customize the asp.net core module uncomment and edit the following section. 
  For more info see https://go.microsoft.com/fwlink/?linkid=838655 -->
  <!--
  <system.webServer>
    <handlers>
      <remove name="aspNetCore"/>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModule" resourceType="Unspecified"/>
    </handlers>
    <aspNetCore processPath="%LAUNCHER_PATH%" arguments="%LAUNCHER_ARGS%" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" />
  </system.webServer>
  -->

  <system.web>
    <globalization
        culture="fr"
        uiCulture="fr" />
  </system.web>

  <system.webServer>
    <httpErrors>
      <remove statusCode="403" />
      <remove statusCode="404" />
      <remove statusCode="405" />
      <remove statusCode="500" />
      
      <error statusCode="404" subStatusCode="0" path="/Error?c=404" responseMode="ExecuteURL" />
      <error statusCode="403" subStatusCode="0" path="/Error?c=403" responseMode="ExecuteURL" />
      <error statusCode="405" subStatusCode="0" path="/Error?c=405" responseMode="ExecuteURL" />
      <error statusCode="500" subStatusCode="0" path="/Error?c=-1" responseMode="ExecuteURL" />
    </httpErrors>
  </system.webServer>

</configuration>